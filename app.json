{
  "expo": {
    "name": "Vet App",
    "slug": "vet-app",
    "version": "13.1.2",
    "orientation": "portrait",
    "icon": "./mobile/assets/",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./mobile/assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./mobile/assets/mobile-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.inasakiii.vetapp"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "b664616f-a3eb-4cb3-8885-94c355606327"
      }
    },
    "owner": "inasakiii"
  }
}
