{
  "expo": {
    "name": "Vet App",
    "slug": "vet-app",
    "version": "11.8.0",
    "orientation": "portrait",
    "icon": "./mobile/assets/PawLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./mobile/assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./mobile/assets/PawLogo.ico",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kennen.vetapp"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "d2efb776-e4f5-45eb-99df-827f6aedcdde"
      }
    }
  }
}
